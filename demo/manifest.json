{
	"name": "IRC Social Demo Application",
	"description": "Demonstrates functionality of the freedom IRC social provider.",
	"version": "0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
  "sockets":{
    "tcp": {
      "connect": ["*"]
    },
    "tcpServer": {
      "listen": ["*"]
    },
    "udp": {
      "bind": ["*"],
      "send": ["*"]
    }
  },
	"icons": {
		"16": "demo-256.png",
		"128": "demo-256.png"
	}
}
